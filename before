                 transform.pars=FALSE,
                 fixed=c(0 ,NA,NA,NA,NA,0,
                         NA,NA,NA,NA,NA,NA))
acf(modelo6$residuals)
pacf(modelo6$residuals)
modelo6
sd=c(1,.039,.0397,.0411,.0399,1,.0188,.0192,.0219,.0241,.0206,.0176)
coeficientes=abs(modelo6$coef/sd)
coeficientes#ma3

